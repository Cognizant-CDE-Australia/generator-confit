{
  "name": "generator-confit",
  "version": "0.0.0-semantically-released",
  "description": "Yeoman generator for creating the development process, tools and a sample project for current-generation web applications",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/odecee/generator-confit.git"
  },
  "bugs": {
    "url": "https://github.com/odecee/generator-confit/issues"
  },
  "contributors": [
    {
      "name": "Brett Uglow",
      "email": "buglow@odecee.com.au",
      "url": "https://github.com/uglow"
    },
    {
      "name": "Michael Richardson",
      "email": "michael@mjrichardson.com.au",
      "url": "https://github.com/mikrich"
    },
    {
      "name": "Tim Weightman",
      "email": "tweightman@odecee.com.au"
    },
    {
      "name": "Keren Burshtein",
      "email": "kburshtein@odecee.com.au"
    }
  ],
  "engines": {
    "node": ">=4.x"
  },
  "scripts": {
    "commit": "git-cz",
    "updateFixtures": "node lib/updateFixtures.js",
    "test": "npm run updateFixtures && npm run test:unit && npm run test:seq",
    "test:seq": "node test/testRunner.js --sequence --MAX_LOG=true",
    "test:unit": "mocha --reporter list --no-timeouts test/spec/unit/*.spec.js --MAX_LOG=false --no-logo",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "buildTool",
    "generators",
    "lib",
    "projectType",
    "test",
    "*.md"
  ],
  "keywords": [
    "build-system",
    "yeoman-generator",
    "yeoman",
    "webpack"
  ],
  "dependencies": {
    "chalk": "0.5.0",
    "checksum": "0.1.1",
    "ejs": "2.3.4",
    "inquirer": "0.12.0",
    "js-yaml": "3.5.2",
    "lodash": "3.10.1",
    "lodash-deep": "1.6.0",
    "sort-keys": "1.1.1",
    "sort-package-json": "1.4.0",
    "spdx-license-list": "2.1.0",
    "yeoman-generator": "0.22.5"
  },
  "devDependencies": {
    "async": "1.5.2",
    "commitizen": "2.5.0",
    "cz-customizable": "4.0.0",
    "cz-customizable-ghooks": "1.1.2",
    "exact-semver": "1.2.0",
    "freeport": "1.0.5",
    "fs-extra": "0.26.3",
    "ghooks": "1.0.1",
    "jasmine-reporters": "2.1.1",
    "jasmine-spec-reporter": "2.4.0",
    "mocha": "2.3.4",
    "npm-module-checklist": "1.6.0",
    "protractor": "3.2.1",
    "semantic-release": "4.3.5",
    "yeoman-assert": "2.1.2",
    "yeoman-test": "1.1.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "exact-semver",
      "commit-msg": "./node_modules/cz-customizable-ghooks/index.js $2"
    },
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/release/commitMessageConfig.js"
    }
  }
}
