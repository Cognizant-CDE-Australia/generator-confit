{
  "extends": "airbnb/base",
  "env": {
      "browser": true,
      "jasmine": true,
      "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "max-len": [2, 200, 4],
    "object-shorthand": 0,
    "no-multiple-empty-lines": 0,
    "no-var": 0,
    "padded-blocks": 0,
    "vars-on-top": 0
  }
}
